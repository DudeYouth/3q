<view class="index">
  <view class="index-hd">
    <view  bindtap="bindViewTap" class="userinfo">
      <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </view>
  </view>
  <view class="index-bd">
    <view class="page-section">
      <view class="weui-cells">
        <navigator url="/page/me/my-published/my-published" class="navigator">
          <view class="navigator-text">我的发布</view>
          <view class="navigator-arrow"></view>
        </navigator>
        <view   class="navigator">
          <view class="navigator-text" style="position:relative;">
            <text>联系客服</text>
            <button open-type="contact" class="opacityButton"></button>
          </view>
          <view class="navigator-arrow"></view>
        </view>
      </view>
    </view>
  </view>
</view>